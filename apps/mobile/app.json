{
  "expo": {
    "name": "mobile",
    "slug": "spste10-research",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spste10.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.spste10.mobile"
    },
    "scheme": "mobile",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 24,
            "kotlinVersion": "2.1.20",
            "gradleProperties": {
              "systemProp.org.gradle.internal.http.connectionTimeout": "300000",
              "systemProp.org.gradle.internal.http.socketTimeout": "300000",
              "maven.wagon.http.retryHandler.count": "10",
              "org.gradle.workers.max": "4",
              "org.gradle.daemon.idletimeout": "10800000",
              "org.gradle.parallel": "false"
            },
            "extraMavenRepos": [
              "https://repo1.maven.org/maven2/"
            ]
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6f47d756-b5ab-41e2-acf2-678fb775d915"
      }
    }
  }
}

